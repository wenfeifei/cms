@page
@{ Layout = "_Layout"; }

<el-table
  class="headerless"
  :data="parameters"
  style="width: 100%">
  <el-table-column
    width="280">
    <template slot-scope="scope">
      <strong>{{scope.row.key}}</strong>
    </template>
  </el-table-column>
  <el-table-column>
    <template slot-scope="scope">
      {{scope.row.value}}
    </template>
  </el-table-column>
</el-table>

@section Scripts{ <script src="/sitefiles/assets/js/admin/settings/utilitiesParameters.js" type="text/javascript"></script> }